{
  "expo": {
    "name": "Chop N Cart",
    "slug": "chow-now",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chopnow_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sharon14kevin.chopnow",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store",
      "expo-apple-authentication",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d1adb3d8-8946-4705-9290-a97cea489ed7"
      }
    },
    "splash": {
      "image": "./assets/images/chopnow_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.sharon14kevin.chopnow",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/chopnow_logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "NOTIFICATIONS",
        "VIBRATE"
      ]
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/d1adb3d8-8946-4705-9290-a97cea489ed7"
    }
  }
}
